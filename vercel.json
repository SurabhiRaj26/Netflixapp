{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/assets/$1"
        },
        {
            "src": "/(.*)\\.(ico|png|svg|jpg|jpeg)",
            "dest": "/dist/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ]
}